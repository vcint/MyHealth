<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cancelupld"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UploadprescripActivity"
    android:background="#fff"
    >


    <ImageView
        android:id="@+id/previewprescrip"
        android:layout_width="370dp"
        android:layout_height="417dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.39"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.229"
        android:src="@drawable/uploadprescrip"
        />

    <Button
        android:id="@+id/uploadcnf"
        android:layout_width="125dp"
        android:layout_height="74dp"
        android:text="Upload"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.227"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/previewprescrip"
        app:layout_constraintVertical_bias="0.224" />

    <Button
        android:id="@+id/canceluploadbtn"
        android:layout_width="134dp"
        android:layout_height="77dp"
        android:text="cancel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.356"
        app:layout_constraintStart_toEndOf="@+id/uploadcnf"
        app:layout_constraintTop_toBottomOf="@+id/previewprescrip"
        app:layout_constraintVertical_bias="0.211" />
</androidx.constraintlayout.widget.ConstraintLayout>